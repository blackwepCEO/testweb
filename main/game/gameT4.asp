var bonus_folder_3 = 1.03;
var bonus_folder_4 = 1.05;
var bonus_folder_5 = 1.07;



function checkCart(p_tdid,p_tdcls,p_idx,p_home_team,p_away_team,p_div_1,p_div_2,p_div_3,p_type,p_selnum,p_gm_code,p_gm_sports, p_handi_key,p_gm_idx)
{


	
	if($("#"+p_tdid).data("status") != "1") return;

	var p_cart_count = $("#cart .cart_node").length;
	var p_temp_val,p_temp_array,p_temp_idx,p_temp_cnt,p_temp_sel, p_temp_code;
	var p_cart_data, p_check_count;
	var strGame_type, nGame_Bedang = nGame_Bedang;
	var backgroundimg_out = "/images/common/off.png";
	var backgroundimg = "/images/common/on.png";
	var nGame_Bedang = eval("p_div_" + p_selnum);


	
	p_div_1 = $("#gm_h_" + p_gm_idx).text();
	if($("#gm_d_" + p_gm_idx).text() != "") p_div_2 = $("#gm_d_" + p_gm_idx).text();
	p_div_3 = $("#gm_a_" + p_gm_idx).text();


	if(p_cart_count > 0)
	{
		p_check_count = 0;
		jQuery.each($("#cart .cart_node"), function(i) {

			p_temp_val = $(this).attr("atrr_value");
			p_temp_array = p_temp_val.split('/');					
			p_temp_idx= p_temp_array[0];
			p_temp_cnt = p_temp_array[1];
			p_temp_sel = p_temp_array[2];
			p_temp_code = p_temp_array[4];
			p_temp_handi_key = p_temp_array[7];
			p_temp_gm_idx = p_temp_array[8];


			
			if( (Number(p_temp_idx) == Number(p_idx) && Number(p_temp_sel) != Number(p_selnum)) || (p_handi_key != p_temp_handi_key && p_temp_cnt == p_type && p_gm_code == p_temp_code) )
			{
				if(getIs1X2Game(p_type))
				{
					if(p_selnum == "1")
					{
						strGame_type = "홈승";
						nGame_Bedang = p_div_1;
					} else if(p_selnum == "2") {
						strGame_type = "무승부";
						nGame_Bedang = p_div_2;
					} else if(p_selnum == "3") {
						strGame_type = "원정승";
						nGame_Bedang = p_div_3;
					}

				} else if(p_type=="sc03") {
					if(p_selnum == "1" )
					{
						strGame_type = "승무";
						nGame_Bedang = p_div_1;
					} else if(p_selnum == "2") {
						strGame_type = "승패";
						nGame_Bedang = p_div_2;
					} else if(p_selnum == "3") {
						strGame_type = "무패";
						nGame_Bedang = p_div_3;
					}
				} else if(p_type=="ba15") {
					if(p_selnum == "1" )
					{
						strGame_type = "연장";
						nGame_Bedang = p_div_1;
					} else if(p_selnum == "3") {
						strGame_type = "미연장";
						nGame_Bedang = p_div_3;
					}
				} else if(getIsOuGame(p_type)) {
					if(p_selnum == "1")
					{
						strGame_type = "오버";
						nGame_Bedang = p_div_1;
					} else if(p_selnum == "3") {
						strGame_type = "언더";
						nGame_Bedang = p_div_3;
					}

				} else if(getIsOeGame(p_type)) {
					if(p_selnum == "1" )
					{
						strGame_type = "홀";
						nGame_Bedang = p_div_1;
					} else if(p_selnum == "3") {
						strGame_type = "짝";
						nGame_Bedang = p_div_3;
					}
				} else if(getIsScoreGame(p_type)) {
					if(p_selnum == "1" )
					{
						strGame_type = "득점";
						nGame_Bedang = p_div_1;
					} else if(p_selnum == "3") {
						strGame_type = "무득점";
						nGame_Bedang = p_div_3;
					}

				} else if(p_type=="sc20") {			
					strGame_type = "예상 스코어";
					nGame_Bedang = p_div_1;

				} else {
