function calcu()
{
	var my_betting_min					= filterNum($("#my_betting_min").text());				
	var my_betting_max					= filterNum($("#my_betting_max").text());				
	var my_result_max						= filterNum($("#my_result_max").text());				

	var my_div_sum						= 0;																	
	var p_my_div_sum					= 0;																	
	var my_result_money					= 0;																	
	var my_betting_money				= $("#my_betting_money").val();							
	var nChk13								= 1;																	
	var p_cart_count						= $("#cart .cart_node").length;							
	var nDivSum								= 0;																	

	jQuery.each($("#cart .cart_node"), function(i) {
			p_temp_val = $(this).attr("atrr_value");
			p_temp_array = p_temp_val.split('/');															
			p_temp_div= p_temp_array[3];

			if(p_temp_div > 0)
			{
				if(i == 0)
				{
					nDivSum = p_temp_div;
				} else {
					nDivSum = nDivSum * p_temp_div;
				}
			}
			if(Number(p_temp_div) < 1.3) nChk13 = 0;
	});

	if ((p_cart_count == 3 || p_cart_count == 4) && nChk13 == 1) 
	{
		nDivSum = nDivSum * bonus_folder_3;
		$(".bn1").show();$(".bn2").hide();$(".bn3").hide();

	} else if (p_cart_count >= 5 && nChk13 == 1)  {
		nDivSum = nDivSum * bonus_folder_5;
		//$(".bn1").hide();$(".bn2").hide();$(".bn3").show();
	} else {
		$(".bn1").hide();$(".bn2").hide();$(".bn3").hide();
	}

	if(nDivSum > 0)
	{
		nDivSum = Number(nDivSum);
		my_div_sum = roundXL(nDivSum, 2).toFixed(2);			
		p_my_div_sum = 	nDivSum;										
		my_betting_money = filterNum(my_betting_money)

		if(!isNaN(my_betting_money))
		{
			my_betting_money = p_my_div_sum * my_betting_money;
			my_betting_money = roundXL(my_betting_money, 2).toFixed();

			if(Right(my_betting_money,1) == "5" && String(p_my_div_sum).indexOf(".") != -1) 	
			{
				my_betting_money = my_betting_money.substring(0,my_betting_money.length-1) + "" + "0";
			}
		} else {
			my_betting_money = 0;
		}
	} else {
		my_betting_money = 0;
	}
	
	$("#max_div_sum").val(p_my_div_sum);
	$("#my_div_sum").text(my_div_sum);
	$("#my_div").val(my_div_sum);			
	$("#my_result_money").text(Java_MoneyFormat(my_betting_money));
	$("#my_result").val(Java_MoneyFormat(my_betting_money));	

	$("#bet_count").text($("#cart .cart_node").length);
	$("#bet_count2").text($("#cart .cart_node").length);
	
	//alert(roundXL(nDivSum, 2).toFixed(2));

}


